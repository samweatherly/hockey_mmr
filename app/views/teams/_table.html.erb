<table class='table table-striped'>
  <thead>
    <tr class="ubuntu">
      <th></th>
      <th>Team</th>
      <th>Rating</th>
    </tr>
  </thead>
  <tbody>
    <% @filter.each do |t| %>
      <tr class="ubuntu">
        <td><%= image_tag(t.logo.url(:teeny)) %></td>
        <td><%= t.name %></td>
        <td><%= t.mmr.round(0) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
